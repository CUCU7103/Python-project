# 클래스의 포함 : 로또 번호 출력
import random

class LottoBall : 
    def __init__ (self, num):
        self.num = num

class LottoMachine:
    def __init__(self):
        self.ballList = []
        for i in range(1,46):
            self.ballList.append(LottoBall(i)) # 포함관계
            
    def selectBall(self):
        #섞기 전 볼 출력
        for a in range(45):
            print(self.ballList[a].num, end =' ')
        print()
       for a in range(45);
            print(self.ballList[a].num, end =' ')
            
            
        random.shuffle(self.ballList) # 섞는다.