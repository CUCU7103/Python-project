# 다중 상속
class Tiger: 
    data = "호랑이 세계"
    
    def Cry(self):
        print('호랑이 : 어흥')
    
    def Eat(self):
        print('맹수는 생고기를 좋아함.')
        
class Lion:
    
    def Cry(self):
        print('사자 : 으르렁')
    
    def Hobby(self):
        print('백수의 왕은 프로그래밍')
        
class Liger1(Tiger,Lion): # 맴버가 중복됬을 시 먼저 적은 클래스가 우선순위를 가진다. 
    pass

lig1 = Liger1()
lig1.Cry()
lig1.Eat()
lig1.Hobby()